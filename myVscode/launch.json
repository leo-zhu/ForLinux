{
    "configurations": [
        {
            "name": "C/C++: g++ debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tb/networktest",
            "args": ["top.dot"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                { "name" : "GLOG_log_dir", "value" : "" },
                { "name" : "GLOG_logtostderr", "value" : true },
                { "name" : "GLOG_minloglevel", "value" : 0 },
                { "name" : "GLOG_stderrthreshold", "value" : 3 },
                { "name" : "GLOG_v", "value" : 7 },
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "${workspaceFolder}/.vscode/mygdb"
        }
    ],
    "version": "2.0.0"
}