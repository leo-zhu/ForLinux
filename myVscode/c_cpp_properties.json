{
    "env": {
        "myDefaultIncludePath": ["${workspaceFolder}", "${workspaceFolder}/include"],
        "myCompilerPath": "/tools/bin/gnu/gcc-11.2.0/bin/g++"
    },
    "configurations": [
        {
            "name": "linux",
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../../thirdparty/systemc/2.3.4/**",
                "${workspaceFolder}/../../thirdparty/pybind11/**",
                "${workspaceFolder}/../../thirdparty/nlohmann/json/**",
                "/tools/bin/gnu/gcc-11.2.0/**",
                "/usr/include"
            ],
            "cppStandard": "c++17",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "browse": {
                "path": ["${workspaceFolder}"],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}
